     -->


    


    <!-- https://w3id.org/won/message#ActivateMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#ActivateMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message that activates an Atom.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>ActivateMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#AtomHintMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#AtomHintMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message sent by a matching service to an Atom in order to inform its owner of other Atoms possibly interesting for establishing a connection, or just generally of interest.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>AtomHintMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#AtomMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#AtomMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Message sent between an atom&apos;s owner and the WoN node hosting the atom. The message is informative only
                   and has no side effect The message is not routed to any other participant.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>AtomMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#ChangeNotificationMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#ChangeNotificationMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message sent from a WoN Node to indicate that the remote Atom has changed.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>ChangeNotificationMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#CloseMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#CloseMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Type of message that closes a Connection.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>CloseMessage</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://w3id.org/won/message#ConnectMessage -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/message#ConnectMessage">
        <rdf:type rdf:resource="https://w3id.org/won/message#MessageType"/>
