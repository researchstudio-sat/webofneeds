        <rdfs:range rdf:resource="https://w3id.org/won/core#AtomContainer"/>
        <rdfs:comment>The Node&apos;s atoms, in a LDP-pageable container.</rdfs:comment>
        <rdfs:label>atomList</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/core#atomState -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/core#atomState">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/won/core#Atom"/>
        <rdfs:range rdf:resource="https://w3id.org/won/core#AtomState"/>
        <rdfs:comment xml:lang="en">Defines the Atom object&apos;s state.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/core"/>
