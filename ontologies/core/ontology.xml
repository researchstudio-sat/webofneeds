        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/won/core#Connection"/>
        <rdfs:range rdf:resource="https://w3id.org/won/core#Connection"/>
        <rdfs:comment xml:lang="en">Links a Connection object to its remote counterpart.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/core"/>
        <rdfs:label>targetConnection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/core#targetSocket -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/core#targetSocket">
        <rdfs:domain rdf:resource="https://w3id.org/won/core#Connection"/>
        <rdfs:range rdf:resource="https://w3id.org/won/core#Socket"/>
        <rdfs:comment xml:lang="en">Indicates that a remote atom or remote connection supports the specified socket (=behavior) or specifies the remote socket to connect with.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/core"/>
        <rdfs:label>targetSocket</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/core#uriPrefixSpecification -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/core#uriPrefixSpecification">
