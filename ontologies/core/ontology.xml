        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/core"/>
        <rdfs:label>flag</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/core#messageContainer -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/core#messageContainer">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/won/core#Atom"/>
                    <rdf:Description rdf:about="https://w3id.org/won/core#Connection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/won/core#MessageContainer"/>
        <rdfs:comment xml:lang="en">Links the Connection object to a container containing connection events (messages).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/core"/>
        <rdfs:label>messageContainer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/core#seeks -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/core#seeks">
        <rdfs:domain rdf:resource="https://w3id.org/won/core#Atom"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>This optional property is one of the two properties that defines the content of every atom (the other is the &quot;is&quot; property). It is used to describe that part of the content that is
sought to fulfill the atom in contrast to the part that is available.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/core"/>
        <rdfs:label>seeks</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/core#socket -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/core#socket">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/won/core#Atom"/>
                    <rdf:Description rdf:about="https://w3id.org/won/core#Connection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/won/core#Socket"/>
        <rdfs:comment xml:lang="en">Indicates that an atom or connection supports the specified socket (=behavior).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/core"/>
        <rdfs:label>socket</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/core#socketDefinition -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/core#socketDefinition">
        <rdfs:domain rdf:resource="https://w3id.org/won/core#Socket"/>
        <rdfs:range rdf:resource="https://w3id.org/won/core#SocketDefinition"/>
        <rdfs:comment xml:lang="en">Links the socket to its definition. A socket must have exactly one definition.</rdfs:comment>
        <rdfs:label>socketDefinition</rdfs:label>
    </owl:ObjectProperty>
    


