        <rdfs:comment xml:lang="en">Links the Atom object to its connections.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/core"/>
        <rdfs:label>connections</rdfs:label>
    </owl:ObjectProperty>
    


