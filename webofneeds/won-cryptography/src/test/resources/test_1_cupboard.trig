              ];

          gr:height
            [ a   gr:QuantitativeValueFloat ;
              gr:hasUnitOfMeasurement "CMT"^^xsd:string ;
              gr:hasValue "45"^^xsd:float
              ];

            gr:depth
              [ a   gr:QuantitativeValueFloat ;
                gr:hasUnitOfMeasurement "CMT"^^xsd:string ;
                gr:hasValue "15"^^xsd:float
                ]
        ]
      ] ;
