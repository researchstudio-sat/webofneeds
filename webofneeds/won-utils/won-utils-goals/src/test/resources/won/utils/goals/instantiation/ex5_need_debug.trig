ex1:p1g-data {
   ex1:ride1 a taxi:Ride .
   ex1:ride1 taxi:hasDriver need:8uiuri25l2b84oywmgsy . # note: p1 links her own identifier to the structure
}

ex1:p1g-shapes {

ex1:selfShape
    a sh:NodeShape ;
    sh:targetNode need:8uiuri25l2b84oywmgsy ;
    sh:property [
        sh:path [ sh:inversePath taxi:hasDriver ];
        sh:class taxi:Ride ;
    ] .

ex1:TaxiRideShape
	a sh:NodeShape ;
    sh:targetClass taxi:Ride ;
	sh:property [
		sh:path dc:title ;             # => triple in other need content (not goals)
		sh:minCount 1 ;
		sh:maxCount 1 ;
        sh:hasValue "taxi in Vienna" ;
	] ;
	sh:property [
		sh:path taxi:hasPickupTime;     # => triple in conversation
		sh:minCount 1 ;
		sh:maxCount 1 ;
        sh:datatype xsd:dateTime;
	] ;
	sh:property [
		sh:path taxi:hasDriver;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:hasValue need:8uiuri25l2b84oywmgsy ;    # p1's shape ties her own identifier to the data
	] ;
	sh:ignoredProperties ( rdf:type ) ;
	sh:closed true .
}