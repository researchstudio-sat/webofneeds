<https://w3id.org/won/ext/hold> rdf:type owl:Ontology ;
                                 owl:versionIRI <https://w3id.org/won/ext/hold/1.0> ;
                                 owl:imports <https://w3id.org/won/core> ;
                                 dc:abstract "This ontology contains the definition of the *HolderSocket* and the *HoldableSocket*, the *holds* and the *heldBy* properties. These express control relationships in WoN. The Atoms holding other atoms represent agents (users or programs) and can themselves not be held."@en ;
                                 dc:creator "Florian Kleedorfer" ;
                                 dc:date "2019-04-15"^^xsd:date ;
                                 dc:title "Web of Needs Hold Extension" ;
                                 dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
                                 vann:preferredNamespacePrefix "wx-hold" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/abstract
dc:abstract rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#labels
rdfs:labels rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/won/ext/hold#heldBy
:heldBy rdf:type owl:ObjectProperty ;
        rdfs:domain won:Atom ;
        rdfs:range won:Atom ;
        owl:propertyDisjointWith :holds .


###  https://w3id.org/won/ext/hold#holds
:holds rdf:type owl:ObjectProperty ;
       rdfs:domain won:Atom ;
       rdfs:range won:Atom .


#################################################################
#    Individuals
#################################################################

###  https://w3id.org/won/ext/hold#HoldableSocket
:HoldableSocket rdf:type owl:NamedIndividual ,
                         won:SocketDefinition ;
                won:compatibleSocketDefinition :HolderSocket ;
                won:derivesAtomProperty :heldBy ;
                won:autoOpen "false"^^xsd:boolean ;
                won:socketCapacity 1 ;
                rdfs:comment "An Atom uses a Socket with this configuration to connect to the Persona Atom that holds it."@en ;
                rdfs:label "HoldableSocket" .


###  https://w3id.org/won/ext/hold#HolderSocket
:HolderSocket rdf:type owl:NamedIndividual ,
                       won:SocketDefinition ;
              won:compatibleSocketDefinition :HoldableSocket ;
              won:derivesAtomProperty :holds ;
              won:autoOpen "false"^^xsd:boolean ;
              rdfs:comment "A Persona Atom uses a Socket with this configuration to connect to Atoms it holds."@en ;
              rdfs:label "HolderSocket" .


#################################################################
#    Annotations
#################################################################

:heldBy rdfs:label "heldBy" ;
        rdfs:comment "Links an Atom to the Atom representing the agent (a person, organization, bot, ...) that controls both."@en .


:holds rdfs:label "holds" ;
       rdfs:comment "Links the Atom representing an agent (a person, organization, bot, ...) to another Atom they control."@en .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :HoldableSocket
                        :HolderSocket
                      )
] .


